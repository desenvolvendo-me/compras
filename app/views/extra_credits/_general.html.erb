<div class="yui3-g">
  <div class="yui3-u-4-5">
    <%= f.input :entity %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :year %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :regulatory_act %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :regulatory_act_type, :disabled => true %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :publication_date, :disabled => true, :input_html => { :value => f.object.decorator.publication_date } %>
  </div>
</div>

<%= f.input :credit_type %>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :extra_credit_nature %>
  </div>

  <div class="yui3-u-1-2">
    <div class="yui3-g">
      <div class="yui3-u-2-3">
        <%= f.input :kind, :disabled => true, :input_html => { :value => f.object.kind_humanize } %>
      </div>

      <div class="yui3-u-1-3">
        <%= f.input :credit_date %>
      </div>
    </div>
  </div>
</div>
