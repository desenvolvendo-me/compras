<%= f.input :supply_order_id, as: :hidden %>
<%= f.input :licitation_process_ratification_item_id, as: :hidden, input_html: { value: ratification_item_id } %>

<div class="yui3-g">
  <div class="yui3-u-7-8">
    <%= f.input :material, disabled: true, input_html: { value: material }  %>
  </div>

  <div class="yui3-u-1-8">
    <%= f.input :reference_unit, disabled: true, input_html: { value: reference_unit } %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-8">
    <%= f.input :quantity, disabled: true, input_html: { value: quantity } %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :authorized_quantity, disabled: true, input_html: { value: authorized_quantity }  %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :balance, disabled: true, input_html: { value: balance }  %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :authorization_quantity %>
  </div>
</div>
