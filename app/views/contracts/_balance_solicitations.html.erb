<% f.object.solicitations.try(:each) do |solicitation| %>
  <h1><%= "Solicitação #{PurchaseSolicitation.find(solicitation[0]).to_s} - #{PurchaseSolicitation.find(solicitation[0]).department}" %></h1>
  <table class="records">
    <thead>
    <tr>
      <th><%= "Material" %></th>
      <th><%= "Qtd Solicitada" %></th>
    </tr>
    </thead>
    <tbody id="ratification_items">
    <% solicitation[1].each do |material| %>
      <tr class="ratification_item inherited nested">
        <td><%= Material.find(material).description %></td>
        <td><%= PurchaseSolicitationItem.joins { purchase_solicitation }.where { purchase_solicitation.id.eq(solicitation[0]) }.where { material_id.eq(material) }.last.quantity %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<% end %>
