<h2 id="title">
  <span><%= "Contrato: #{resource.contract_number} / #{resource.year}" %></span>
  <span class="subtitle"><%= resource.contract_type %></span>
  <span class="subtitle"><%= "Início da Vigência: #{resource.start_date} - Término da Vigência: #{resource.end_date}" %></span>
</h2>

<%= render 'subtitle' %>

<%= render 'form_wrapper' %>

<style>
  h2 span{
    display: block;
  }
  h2 span.subtitle{
    font-size: 12px;
  }
</style>