<tr class="ratification_item inherited nested">
  <td align="center">
    <%= f.check_box :ratificated %>

    <%= f.input :id, :as => 'hidden' %>

    <% if resource.licitation_process_licitation? %>
      <% if resource.has_realignment_price? %>
        <%= f.input :realignment_price_item_id, :as => 'hidden',
          :input_html => { :value => f.object.decorator.realignment_price_item_id_or_mustache_variable } %>
      <% else %>
        <% if resource.licitation_process_trading? %>
          <%= f.input :purchase_process_trading_item_id, :as => 'hidden',
            :input_html => { :value => f.object.decorator.trading_item_id_or_mustache_variable } %>
        <% else %>
          <%= f.input :purchase_process_creditor_proposal_id, :as => 'hidden',
            :input_html => { :value => f.object.decorator.creditor_proposal_id_or_mustache_variable } %>
        <% end %>
      <% end %>
    <% end %>

    <%= f.input :purchase_process_item_id, :as => 'hidden',
                :input_html => { :value => f.object.decorator.purchase_process_item_id_or_mustache_variable } %>

    <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>
  </td>

  <td align="right"><%= f.object.decorator.code_or_mustache_variable %></td>
  <td class="description"><%= f.object.decorator.description_or_mustache_variable %></td>
  <td align="right"><%= f.object.decorator.reference_unit_or_mustache_variable %></td>
  <td align="right"><%= f.object.decorator.quantity_or_mustache_variable %></td>
  <td align="right"> <%= f.object.decorator.unit_price_or_mustache_variable %></td>
  <td align="right"> <%= f.object.decorator.total_price_or_mustache_variable %></td>
</tr>
