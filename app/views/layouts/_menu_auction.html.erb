
  <%= render 'layouts/menus/legal_peoples' %>
  


<script>
  $("#menu li").click(function (event) {
    $(this).addClass("active");

    if ($(event.target).is("a[href=#]")) {
      event.preventDefault();
    }
  });

  $("#menu li").mouseout(function () {
    $(this).removeClass("active");
  });
</script>
