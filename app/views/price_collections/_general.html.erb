<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :collection_number, :disabled => true %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :year %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :date %>
  </div>
</div>

<%= f.input :status, :disabled => f.object.new_record? %>
<%= f.input :type_of_calculation %>
<%= f.input :delivery_location %>
<%= f.input :employee %>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :payment_method %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :period %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :period_unit %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :proposal_validity %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :proposal_validity_unit %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :expiration %>
  </div>
</div>

<%= f.input :object_description %>
<%= f.input :observations %>
