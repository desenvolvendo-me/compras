<tr data-value="<%= record.id %>" data-label="<%= record %>" data-cpf="<%= record.cpf %>" data-phone="<%= record.phone %>" data-mobile="<%= record.mobile %>" data-type="<%= record.class.to_s %>" data-modal-info-url="/people/<%= record.id %>.js" data-street="<%= record.address.try(:street).try(:name) %>" data-street-id="<%= record.address.try(:street).try(:id) %>" data-number="<%= record.address.try(:number) %>" data-block="<%= record.address.try(:block) %>" data-room="<%= record.address.try(:room) %>" data-complement="<%= record.address.try(:complement) %>" data-neighborhood="<%= record.address.try(:neighborhood).try(:name) %>" data-neighborhood-id="<%= record.address.try(:neighborhood).try(:id) %>" data-condominium="<%= record.address.try(:condominium).try(:name) %>" data-condominium-id="<%= record.address.try(:condominium).try(:id) %>" data-land-subdivision="<%= record.address.try(:land_subdivision) %>" data-land-subdivision-id="<%= record.address.try(:land_subdivision).try(:id) %>" data-district="<%= record.address.try(:district).try(:name) %>" data-district="<%= record.address.try(:district).try(:id) %>" data-city="<%= record.address.try(:city).try(:name) %>" data-zip-code="<%= record.address.try(:zip_code) %>" data-state="<%= record.address.try(:state).try(:name) %>">
  <% attributes.each do |attribute| %>
    <td><%= record.send attribute %></td>
  <% end %>
</tr>
