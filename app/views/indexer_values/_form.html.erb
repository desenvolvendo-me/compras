<div class="yui3-g indexer-value nested">
  <div class="yui3-u-1-6">
    <%= f.input :date %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :value, :input_html => { :value => f.object.decorator.value } %>
  </div>

  <div class="yui3-u-1-3">
    <div class="nested-remove">
      <input type="button" class="button negative remove-indexer-value" value="Remover">
    </div>
  </div>

  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>
</div>
