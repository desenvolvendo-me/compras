<tr class="record nested-record item-record">
  <td><%= material %></td>
  <td><%= f.input :manufacturer, input_html:{class: 'form-control form-control-sm'}, label: false%></td>
  <td><%= f.input :model_version, input_html:{class: 'form-control form-control-sm'}, label: false%></td>
  <td><%= f.input :quantity, input_html:{class: 'form-control form-control-sm'}, label: false%></td>
  <td><%= f.input :unit_price, input_html:{value: number_with_delimiter(f.object.unit_price), class: 'form-control form-control-sm' }, label: false%></td>
  <td>
    <%= f.input :global_price, input_html:{value: number_with_delimiter(f.object.global_price), class: 'form-control form-control-sm', readonly: true}, label: false%>

    <%= f.input :description, as: :hidden%>
    <%= f.input :auction_item_id, as: :hidden  %>
    <%= f.input :id, as: :hidden  %>
  </td>

</tr>

