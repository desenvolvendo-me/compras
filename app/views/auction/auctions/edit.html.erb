<div class="wrapper-form">
  <h2>
    <span><%= "Pregão: #{resource.licitation_number} / #{resource.year}" %></span>
    <span class="subtitle"><%= resource.object %></span>
    <span class="subtitle"><%= "Entrega da Proposta: #{resource.proposal_delivery}" %></span>
    <span class="subtitle"><%= "Abertura da Licitação: #{resource.bid_opening.strftime('%d/%m/%Y')}" %></span>
    <span class="subtitle"><%= "Pregoiero: #{resource.employee&.name}" %></span>
  </h2>
</div>

<%= render 'subtitle' %>

<%= render 'form_wrapper' %>

<style>
  h2 span{
    display: block;
  }
  h2 span.subtitle{
    font-size: 19px;
  }
</style>