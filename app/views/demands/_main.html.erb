<div class="yui3-g">
  <div class="yui3-u-1-6">
    <%= f.input :year %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :description %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :status, :disabled => f.object.id.nil? %>
  </div>
</div>

<%= f.input :observation %>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :initial_date %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :final_date %>
  </div>
</div>
