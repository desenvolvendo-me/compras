<div class="member nested">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-g">
    <div class="yui3-u-1-2">
      <%= f.input :individual, :input_html => { :class => 'individual' } %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :registration %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :role %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :role_nature %>
    </div>
  </div>

  <p class="yui3-u">
    <input type="button" class="button negative remove-member" value="Remover Membro">
  </p>
</div>
