<div class="member inherited nested">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <%= f.input :licitation_commission_member_id, :as => 'hidden', :input_html => { :value => f.object.decorator.licitation_commission_member_id_or_mustache_variable } %>

  <div class="yui3-g">
    <div class="yui3-u-1-2">
      <%= f.input :individual_name, :disabled => true, :input_html => { :value => f.object.decorator.individual_name_or_mustache_variable }  %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :role_humanize, :disabled => true, :input_html => { :class => 'role', :value => f.object.decorator.role_humanize_or_mustache_variable } %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :role_nature_humanize, :disabled => true, :input_html => { :class => 'role-nature', :value => f.object.decorator.role_nature_humanize_or_mustache_variable } %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :registration, :required => false, :disabled => true, :input_html => { :value => f.object.decorator.registration_or_mustache_variable } %>
    </div>
  </div>

  <p class="yui3-u">
    <input type="button" class="button negative remove-member" value="Remover Membro">
  </p>
</div>
