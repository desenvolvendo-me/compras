<div class="pledge-parcel">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <h3>Parcela <span class="order"><%= f.object.number %></span></h3>

  <div class="yui3-g nested">
    <div class="yui3-u-1-3">
      <%= f.input :expiration_date, :input_html => { :class => :expiration_date } %>
    </div>

    <div class="yui3-u-1-3">
      <%= f.input :value, :input_html => { :class => :value } %>
    </div>

    <div class="yui3-u-1-3 nested-remove">
      <input type="button" class="button negative remove-pledge-parcel" value="Remover Vencimento">
    </div>
  </div>
</div>
