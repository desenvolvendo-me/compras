<%= f.input :kind %>
<%= f.input :value %>

<table id="month" class="records">
  <tbody>
    <% (1..12).each do |month| %>
      <tr>
        <td>
          <%= I18n.t('date.month_names')[month] %>
        </td>
        <td class="value">
          0,00
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
