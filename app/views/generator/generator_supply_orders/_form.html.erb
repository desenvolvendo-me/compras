<%= f.input :user_id, as: :hidden, input_html: { value: current_user.id, class: 'user-id' } %>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :status_humanize, as: :string, disabled: true %>
  </div>
</div>

<%= render "supply_requests", f: f %>

<% if f.object.processed_with_errors? %>
  <%= f.input :error_message, disabled: true %>
<% end %>
