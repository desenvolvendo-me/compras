<%= f.association :personable, :collection => f.object.personable.is_a?(Company) ? f.object.personable.localized : Company.new do |c| %>
  
  

    <div id="company">
      <%= c.input :choose_simple %>
      <div class="yui3-g">
        <div class="yui3-u-2-3">
          <%= f.input :name %>
        </div>
        <div class="yui3-u-1-3">
          <%= c.input :cnpj %>
        </div>
      </div>

      <div class="yui3-g">
        <div class="yui3-u-1-3">
          <%= c.input :state_registration %>
        </div>

        <div class="yui3-u-1-3">
          <%= c.input :uf_state_registration %>
        </div>
        <div class="yui3-u-1-3">
          <%= c.input :main_cnae %>
        </div>
      </div>
      <div class="yui3-g">
        <div class="yui3-u-1-2">
            <%= c.input :responsible_name %>
        </div>
        <div class="yui3-u-1-2">
            <%= c.input :responsible_role %>
        </div>
      </div>
      

 

    <input type="hidden" name="<%= c.sanitized_object_name %>[cnae_ids][]" />
      
    </div>

     <div id="company_address">
      <%= f.association :address, :collection => f.object.address || Address.new do |a| %>
        <%= render "addresses/address", :a => a %>
      <% end %>

    </div>


    <div id="company_contact">
      <div class="yui3-g">
        <div class="yui3-u-1-3">
          <%= f.input :phone, :as => :string %>
        </div>

        
        <div class="yui3-u-2-3">
          <%= f.input :email %>
        </div>
        
      </div>
     
     
    </div>

   
    
<% end %>
