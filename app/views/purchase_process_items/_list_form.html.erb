<tr class="record document-type">
  <td class='id'>
    <%= id %>
  </td>
  <td>
    <%= material %>
  </td>
  <td>
    <%= quantity %>
  </td>
  <td>
    <%= unit_price %>
  </td>
  <td>
    <input id="<%= f.sanitized_object_name %>_purchase_process_items_record_<%= id %>_id" type="hidden" name="<%= f.sanitized_object_name %>[purchase_process_item_ids][]" value="<%= id %>" />
    <%= button_tag 'Remover', :type => :button,  :class => 'button negative modal-finder-remove' if f.object.licitation_process_updatable? %>
  </td>
</tr>
