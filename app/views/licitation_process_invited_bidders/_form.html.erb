<fieldset class="licitation-process-invited-bidder">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-g">
    <div class="yui3-u-1-5">
      <%= f.input :provider %>
    </div>

    <div class="yui3-u-1-5">
    <%= f.input :protocol  %>
    </div>

    <div class="yui3-u-1-5">
    <%= f.input :protocol_date, :input_html => { :class => 'protocol-date' } %>
    </div>

    <div class="yui3-u-1-5">
    <%= f.input :receipt_date, :input_html => { :class => 'receipt-date' } %>
    </div>

    <div class="yui3-u-1-5">
    <%= f.input :auto_convocation, :input_html => { :class => 'auto-convocation' } %>
    </div>
  </div>

  <div class="nested-remove">
    <input type="button" class="button negative remove-licitation-process-invited-bidder" value="Remover Licitante">
  </div>
</fieldset>
