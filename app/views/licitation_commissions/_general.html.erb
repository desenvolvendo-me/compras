<%= f.input :commission_type %>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :regulatory_act %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :regulatory_act_publication_date, :disabled => true, :input_html => { :value => f.object.presenter.regulatory_act_publication_date } %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :nomination_date %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :expiration_date %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :exoneration_date %>
  </div>
</div>

<%= f.input :description %>
