<div class="buttons">
  <%= annul_link(:param_name => :purchase_solicitation_id) %>
  <%= f.button :submit unless f.object.annulled? %>
  <% if f.object.persisted? %>
    <%= link_to f.object.decorator.liberation_label, f.object.decorator.liberation_url, :class => 'button primary' %>
  <% end %>
  <%= f.button :cancel %>
</div>
