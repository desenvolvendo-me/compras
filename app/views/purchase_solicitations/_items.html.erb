<div id="item-tab">
  <%= f.error :items %>

  <div class="yui3-g">
    <div class="yui3-u-1-3">
      <%= f.input :total_items_value, :disabled => true,
          :input_html => { :value => number_with_precision(f.object.total_items_value) } %>
    </div>
  </div>

  <%= f.input :items, as: :nested_grid, data_disabled: f.object.decorator.is_annulled_message,
      columns: [:material, :reference_unit, :brand, :quantity, :unit_price, :estimated_total_price] %>
</div>
