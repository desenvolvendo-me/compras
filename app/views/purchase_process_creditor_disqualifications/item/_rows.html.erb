<tr>
  <td><%= proposal.item_lot %></td>
  <td><%= proposal_counter + 1 %></td>
  <td><%= proposal.item %></td>
  <td><%= proposal.material_reference_unit %></td>
  <td class='right'><%= proposal.item_quantity %></td>
  <td class='right'><%= proposal.unit_price %></td>
  <td class='right'><%= proposal.decorator.total_price %></td>

  <td align='center'>
    <%= check_box_tag 'purchase_process_creditor_disqualification[proposal_item_ids][]', proposal.id,
      check_proposal_item?(proposal), { id: "proposal_item_ids_#{proposal.id}", class: :item_check } %>
  </td>
</tr>
