<div class="nested-event-account">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-g">
    <div class="yui3-u-2-5">
      <%= f.input :checking_account_of_fiscal_account %>
    </div>

    <div class="yui3-u-1-4">
      <%= f.input :nature_release %>
    </div>

    <div class="yui3-u-1-5">
      <%= f.input :operation %>
    </div>

    <div class="yui3-u-1-7">
      <input type="button" class="button negative remove-event-account" value="Remover">
    </div>
  </div>
</div>
