<div class="nested-agreement-bank-account">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <%= f.input :bank_account, :modal_info => true %>

  <div class="yui3-g">
    <div class="yui3-u-1-4">
      <%= f.input :creation_date, :disabled => true, :input_html => { :value => f.object.decorator.creation_date } %>
    </div>

    <div class="yui3-u-1-4">
      <%= f.input :desactivation_date, :disabled => true %>
    </div>

    <div class="yui3-u-1-4">
      <%= f.input :status, :disabled => true %>
    </div>
  </div>


  <p class="yui3-u">
    <input type="button" class="button negative remove-agreement-bank-account" value="Remover Conta BancÃ¡ria">
  </p>
</div>
