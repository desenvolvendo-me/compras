<%= f.input :expense_kind %>
<%= f.input :pledge_historic %>
<%= f.input :licitation_modality %>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :licitation_process %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :licitation_process_licitation_number, :disabled => true %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :management_contract %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :management_contract_signature_date, :disabled => true, :input_html => { :value => (l(f.object.management_contract_signature_date) if f.object.management_contract_signature_date) } %>
  </div>
</div>

<%= f.input :description %>

<script>
  $('#pledge_licitation_process_id').change(function(event, data) {
    if (data) {
      var record = data.record;
      $('#pledge_licitation_process_licitation_number').val(record.data('licitation-number'));
    }
    else{
      $('#pledge_licitation_process_licitation_number').val('');
    }
  });
</script>
