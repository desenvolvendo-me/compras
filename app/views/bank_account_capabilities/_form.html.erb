<div class="nested-bank-account-capabilities">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-g">
    <div class="yui3-u-1-3">
      <%= f.input :capability %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :date, :disabled => true %>
    </div>

    <div class="yui3-u-1-8">
      <%= f.input :status, :disabled => true, :input_html => { :class => 'select-status' } %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :inactivation_date, :disabled => true, :input_html => { :class => 'input-date' } %>
    </div>

    <div class="yui3-u-1-6">
      <%= link_to_modal_info 'bank_account_capability_capability', f.object.capability %>
    </div>
  </div>

  <p class="yui3-u">
    <input type="button" class="button negative remove-bank-account-capabilities" value="Remover">
  </p>
</div>
