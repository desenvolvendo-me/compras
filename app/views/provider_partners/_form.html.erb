<div class="yui3-g provider-partner nested">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-u-1-3">
    <%= f.input :individual %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :function %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :date %>
  </div>

  <p class="yui3-u">
    <input type="button" class="button negative remove-provider-partner" value="Remover Sócio / Responsável">
  </p>
</div>
