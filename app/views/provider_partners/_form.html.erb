<div class="yui3-g provider-partner nested">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-u-1-4">
    <%= f.input :individual %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :function %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :date %>
  </div>

  <div class="yui3-u-1-4 nested-remove">
    <input type="button" class="button negative remove-provider-partner" value="Remover">
  </div>
</div>
