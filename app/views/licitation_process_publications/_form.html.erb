<%= f.input :licitation_process_id, :as => 'hidden' %>

<div class="yui3-g" >
  <div class="yui3-u-1-2">
    <%= f.input :name %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :publication_date %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :publication_of, collection: (publication_of_collection(f.object.licitation_process) if f.object.try(:licitation_process)) %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :circulation_type %>
  </div>
</div>
