<h2 id="title">
  <%= resource.decorator.creditors_title %>
</h2>

<div class="actions">
  <ul>
    <li>
      <%= link_to 'Mapa de Propostas', new_report_map_of_proposal_path(licitation_process_id: @licitation_process) unless @creditors.empty? %>
    </li>
    <li>
      <%= link_to 'Voltar ao processo de compra', edit_licitation_process_path(@licitation_process) %>
    </li>
  </ul>
</div>

<table class="records" id="administrative-process-budget-allocation-records">
  <thead>
    <th><%= LicitationProcess.human_attribute_name :creditor %></th>
    <th><%= LicitationProcess.human_attribute_name :creditor_email %></th>
    <th>Propostas</th>
    <th>Desclassificar</th>
    <th>Desclassificada?</th>
  </thead>

  <tbody>
    <%= render partial: 'creditors_rows', collection: @creditors, as: :creditor %>
  </tbody>
</table>
