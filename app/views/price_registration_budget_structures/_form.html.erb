<div class="price-registration-budget-structure nested">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-g">
    <div class="yui3-u-1-3">
      <%= f.input :budget_structure, :input_html => { :class => 'budget-structure-modal' } %>
    </div>

    <div class="yui3-u-1-5">
      <%= f.input :quantity_requested %>
    </div>

    <div class="yui3-u-1-5">
      <%= f.input :budget_structure_balance %>
    </div>

    <div class="yui3-u-1-12">
      <%= f.input :ride, :as => :select %>
    </div>

    <div class="yui3-u-1-6 nested-remove">
      <input type="button" class="button negative remove-price-registration-budget-structure" value="Remover">
    </div>
  </div>
</div>
