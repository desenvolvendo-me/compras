<tr class="record nested-record item-record" id="supply_request-id-<%= supply_request_id %>">
  <td><%= supply_request %></td>
  <td>
    <%= link_to 'Editar', '#', class: 'edit-nested-record' %>
  </td>

  <td>
    <%= f.input :supply_request_id, as: :hidden, input_html: {value: supply_request_id, class: 'supply_request-id'} %>
    <%= f.input :supply_request, as: :hidden, input_html: {value: supply_request} %>
    <%= f.input :id, as: :hidden, input_html: {value: id} %>
    <%= f.input :_destroy, as: :hidden, input_html: {class: 'destroy'} %>

    <%= link_to 'Remover', '#', class: 'remove-nested-record' %>
  </td>
</tr>