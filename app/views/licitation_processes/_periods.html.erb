<div class="yui3-g">
  <div class="yui3-u-1-4">
    <%= f.input :process_date %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :notice_availability_date %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :contact %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :envelope_delivery_date %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :envelope_delivery_time %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :proposal_envelope_opening_date, :input_html => { "data-disabled" => f.object.decorator.disabled_envelope_message,
      :readonly => f.object.decorator.disabled_envelope_message } %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :proposal_envelope_opening_time, :input_html => { "data-disabled" => f.object.decorator.disabled_envelope_message,
      :readonly => f.object.decorator.disabled_envelope_message } %>
  </div>
</div>

<div class="not-fields-to-trading">
  <div class="yui3-g">
    <div class="yui3-u-1-3">
      <%= f.input :authorization_envelope_opening_date %>
    </div>

    <div class="yui3-u-1-5">
      <%= f.input :authorization_envelope_opening_time %>
    </div>
  </div>
</div>

<div class="fields-to-trading">
  <div class="yui3-g">
    <div class="yui3-u-1-3">
      <%= f.input :closing_of_accreditation_date %>
    </div>

    <div class="yui3-u-1-5">
      <%= f.input :closing_of_accreditation_time %>
    </div>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-5">
    <%= f.input :expiration %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :expiration_unit %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :period %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :period_unit %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-5">
    <%= f.input :stage_of_bids_date %>
  </div>
</div>
