<div class="yui3-g">
  <div class="yui3-u-1-4">
    <%= f.input :process, :disabled => true %>
  </div>

  <div class="yui3-u-1-8">
    <%= f.input :year, :disabled => f.object.persisted? %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :process_date %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :administrative_process, :disabled => true %>
  </div>

  <div class="yui3-u-5-24">
    <%= f.input :status, :disabled => true %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :administrative_process_licitation_modality, :disabled => true %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :administrative_process_object_type_humanize, :disabled => true %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :judgment_form %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :administrative_process_responsible, :disabled => true %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :administrative_process_item, :disabled => true %>
  </div>
</div>

<%= f.input :administrative_process_description, :disabled => true, :as => 'text' %>
<%= f.input :price_registration %>
<%= f.input :type_of_calculation %>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :capability %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :pledge_type %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-5">
    <%= f.input :expiration %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :expiration_unit %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :readjustment_index %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-4">
    <%= f.input :licitation_number, :disabled => true %>
  </div>

  <div class="yui3-u-1-8">
    <%= f.input :year, :disabled => true, :input_html => { :id => nil } %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :modality, :disabled => true, :as => :string, :input_html => { :value => f.object.modality_humanize } %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :envelope_delivery_date %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :envelope_delivery_time %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :envelope_opening_date %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :envelope_opening_time %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-5">
    <%= f.input :period %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :period_unit %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :payment_method %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-5">
    <%= f.input :caution_value %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :legal_advice %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :legal_advice_date %>
  </div>

  <div class="yui3-u-1-6">
    <%= f.input :contract_date %>
  </div>

  <div class="yui3-u-1-4">
    <%= f.input :contract_expiration %>
  </div>
</div>

<%= f.input :observations %>

<div class="yui3-g">
  <div class="yui3-u-1-5">
    <%= f.input :ratification_date, :disabled => true, :input_html => { :class => 'important' } %>
  </div>

  <div class="yui3-u-1-5">
    <%= f.input :adjudication_date, :disabled => true, :input_html => { :class => 'important' } %>
  </div>
</div>
