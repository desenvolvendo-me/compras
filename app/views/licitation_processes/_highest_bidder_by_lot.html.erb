<table class="records">
  <thead>
    <tr>
      <th>
        Lote
      </th>
      <th>
        Valor cotado
      </th>
      <th>
        Fornecedor vencedor
      </th>
    </tr>
  </thead>

  <tbody>
    <% localized(resource.licitation_process_lots).each_with_index do |lot, index| %>
      <tr>
        <td>
          <%= lot %>
        </td>
        <td>
          <%= lot.winner_proposals.decorator.proposal_total_value_by_lot(lot.id) %>
        </td>
        <td>
          <%= lot.winner_proposals.creditor %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
