<div class="delivery_schedule">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-g">
    <div class="yui3-u-1-8">
      <%= f.input :sequence, :disabled => true %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :scheduled_date %>
    </div>

    <div class="yui3-u-1-6">
      <%= f.input :delivery_date %>
    </div>

    <div class="yui3-u-1-5">
      <%= f.input :delivery_schedule_status %>
    </div>
  </div>

  <%= f.input :observations %>

  <p class="yui3-u">
    <input type="button" class="button negative remove_item" value="Remover Cronograma de Entrega">
  </p>
</div>
