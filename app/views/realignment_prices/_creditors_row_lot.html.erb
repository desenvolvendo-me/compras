<% proposals(@purchase_process, creditor).each do |proposal| %>
  <tr>
    <td><%= creditor.to_s %></td>
    <td><%= creditor.person_email %></td>
    <td><%= proposal.lot %></td>

    <td>
      <%= link_to realignment_link_title(@purchase_process.id, creditor.id, proposal.lot),
        realignment_path_helper(@purchase_process.id, creditor.id, proposal.lot) %>
    </td>
  </tr>
<% end %>
