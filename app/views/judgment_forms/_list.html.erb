<table class="records">
  <thead>
    <tr>
      <th>
        <%= resource_class.human_attribute_name :id %>
      </th>
      <th>
        <%= resource_class.human_attribute_name :description %>
      </th>
      <th>
        <%= resource_class.human_attribute_name :kind %>
      </th>
      <th>
        <%= resource_class.human_attribute_name :licitation_kind %>
      </th>
    </tr>
  </thead>

  <tbody>
    <% localized(collection) do |resource| %>
      <tr id="<%= dom_id resource %>">
        <td>
          <%= resource.id %>
        </td>
        <td>
          <%= link_to resource.description, edit_resource_path(resource) %>
        </td>
        <td>
          <%= resource.kind_humanize %>
        </td>
        <td>
          <%= resource.licitation_kind_humanize %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
