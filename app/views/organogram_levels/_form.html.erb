<fieldset>
  <div class="yui3-g">
    <div class="yui3-u-1-4">
      <%= f.input :level %>
    </div>

    <div class="yui3-u-1-4">
      <%= f.input :name %>
    </div>

    <div class="yui3-u-1-4">
      <%= f.input :digits %>
    </div>

    <div class="yui3-u-1-4">
      <%= f.input :organogram_separator, :collection => OrganogramSeparator.to_a %>
    </div>
  </div>

  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <p>
    <input type="button" class="button negative remove-organogram_level" value="Remover Estrutura">
  </p>
</fieldset>
