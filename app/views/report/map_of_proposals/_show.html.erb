<h3><%= current_prefecture %></h3>
<h3>Mapa Comparativo de Preços</h3>
<h3><%= resource.licitation_process %></h3>

<% resource.licitation_process.items.each do |item| %>
<table class="records <%= item.material.description.parameterize %>">
  <thead>
    <tr>
      <td><%= "#{item.lot} #{item.material}"%></td>
      <td>Quantidade</td>
      <td><%= item.quantity %></td>
    </tr>
    <tr>
      <td>Fornecedor</td>
      <td>Valor Unitário</td>
      <td>Valor Total</td>
    </tr>
  </thead>
  <tbody>
    <% resource.item_creditor_proposals(item).each do |creditor_proposal|%>
    <% css_class = creditor_proposal.decorator.css_class creditor_proposal%>
    <tr class="<%= css_class %> <%= creditor_proposal.creditor_name.parameterize %>">
      <td><%= "#{creditor_proposal.creditor_cnpj} - #{creditor_proposal.creditor_name}" %></td>
      <td><%= creditor_proposal.decorator.unit_price_with_currency %></td>
      <td><%= creditor_proposal.decorator.total_price_with_currency %></td>
    </tr>
    <% end %>
    <tr>
      <td colspan=3></td>
    </tr>
  </tbody>
</table>
<% end %>
