<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :object_type, :include_blank => true %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :modality, :include_blank => true, wrapper_class: 'modality' %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-2">
    <%= f.input :licitation_commission, as: :modal,
      data_url: modal_licitation_commissions_url, hidden_field: :licitation_commission_id %>
  </div>
</div>

<div class="yui3-g">
  <div class="yui3-u-1-3">
    <%= f.input :start_date, as: :date %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :end_date, as: :date %>
  </div>
</div>
