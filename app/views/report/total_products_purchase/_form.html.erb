<div class="yui3-g">

  <div class="yui3-u-1-3">
    <%= f.input :creditor, as: :auto_complete, required: true,
                hidden_field: :creditor_id,
                source_path: creditors_path(by_purchasing_unit: current_user.id) %>
  </div>

  <div class="yui3-u-1-3">
    <%= f.input :material, as: :auto_complete, required: true,
                hidden_field: :material_id,
                source_path: materials_path(by_licitation_process_status: "approved") %>
  </div>
</div>

<script>
    $(function () {
        $('#new_total_products_purchase_report')[0].reset();
    });
</script>