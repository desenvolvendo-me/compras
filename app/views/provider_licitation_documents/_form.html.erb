<div class="yui3-g provider-licitation-document nested">
  <%= f.input :id, :as => 'hidden' %>
  <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

  <div class="yui3-u-1-2">
    <%= f.input :document_type %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :document_number %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :emission_date %>
  </div>

  <div class="yui3-u-1-2">
    <%= f.input :expiration_date %>
  </div>

  <p class="yui3-u">
    <input type="button" class="button negative remove-provider-licitation-document" value="Remover Documento">
  </p>
</div>
