<fieldset class="pledge-expiration">
  <div class="yui3-g nested">
    <%= f.input :id, :as => 'hidden' %>
    <%= f.input :_destroy, :as => 'hidden', :input_html => { :class => 'destroy' } %>

    <div class="yui3-u-1-3">
      <%= f.input :number, :disabled => true, :input_html => { :class => :order } %>
    </div>

    <div class="yui3-u-1-3">
      <%= f.input :expiration_date %>
    </div>

    <div class="yui3-u-1-3">
      <%= f.input :value %>
    </div>

    <div class="yui3-u-1-7 nested-remove">
      <input type="button" class="button negative remove-pledge-expiration" value="Remover Vencimento">
    </div>
  </div>
</fieldset>
