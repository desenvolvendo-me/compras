<div class="buttons">
    <% if f.object.persisted? %>
      <%= f.button :destroy if can?(:destroy, main_controller_name) %>
      <%= link_to('Licitantes', bidders_path(:licitation_process_id => resource.licitation_process.id), :class => "button primary") %>
      <%= link_to('ATA do Julgamento', trading_path(resource), :class => "button primary") %>
    <% end %>

    <% if can?(:modify, main_controller_name) %>
      <%= submit_tag "Salvar e ir para Itens/Ofertas", :class => "button primary" %>
      <%= f.button :submit %>
    <% end %>

    <%= f.button :back %>
</div>
